<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="setu_rma_reason_tree_view" model="ir.ui.view">
            <field name="name">setu_rma_reason.tree</field>
            <field name="model">setu.return.reason</field>
            <field name="arch" type="xml">
                <tree string="Return Reason" editable="top">
                    <field name="name" required="1"/>
                </tree>
            </field>
        </record>

        <record id="setu_rma_reason_search_view" model="ir.ui.view">
            <field name="name">setu_rma_reason.search</field>
            <field name="model">setu.return.reason</field>
            <field name="arch" type="xml">
                <search string="Return Reason">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="action_setu_return_reason" model="ir.actions.act_window">
            <field name="name">Return Reason</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">setu.return.reason</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Return Reason.
                </p>
            </field>
        </record>

    </data>
</odoo>